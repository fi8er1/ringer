<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relTop"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg_app_new2"
    android:clickable="true">

    <LinearLayout
        android:id="@+id/adView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" />

    <ScrollView
        android:id="@+id/scrol"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/adView"
        android:fillViewport="true"
        android:isScrollContainer="true">

        <RelativeLayout
            android:id="@+id/firstrelinscroll"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <LinearLayout
                android:id="@+id/LinearLayout1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5.0dip"
                android:layout_marginRight="5.0dip"
                android:layout_marginTop="12.0dip"
                android:background="@drawable/txt_bg_hori_new5"
                android:orientation="vertical"
                android:padding="0.0dip"
                android:weightSum="1.0">

                <LinearLayout
                    android:id="@+id/LinearLayout2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5.0dip"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5.0dip"
                        android:text="@string/app_name"
                        android:textAppearance="?android:textAppearanceLarge"
                        android:textColor="@android:color/white"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="70.0dip"
                        android:layout_height="70.0dip"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="15.0dip"
                        android:src="@mipmap/ic_launcher" />

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5.0dip"
                        android:layout_marginRight="5.0dip"
                        android:layout_toRightOf="@id/textView1"
                        android:text="@string/version"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@android:color/black"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="10.0dip"
                        android:text="1.1"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@android:color/black" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/LinearLayout1"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5.0dip"
                android:layout_marginRight="5.0dip"
                android:layout_marginTop="12.0dip"
                android:background="@drawable/txt_bg_hori_new5"
                android:orientation="vertical"
                android:padding="0.0dip"
                android:weightSum="1.0">

                <LinearLayout
                    android:id="@+id/LinearLayout50"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="5.0dip"
                    android:layout_weight="1.0"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15.0dip"
                        android:layout_marginRight="5.0dip"
                        android:layout_marginTop="8.0dip"
                        android:text="@string/libraries"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@id/textView4"
                        android:layout_below="@id/textView4"
                        android:layout_marginLeft="15.0dip"
                        android:layout_marginRight="5.0dip"
                        android:layout_marginTop="2.0dip"
                        android:text="@string/musicg"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@id/textView5"
                        android:layout_below="@id/textView5"
                        android:layout_marginLeft="15.0dip"
                        android:layout_marginRight="5.0dip"
                        android:layout_marginTop="1.0dip"
                        android:text="@string/musicg_explain"
                        android:textAppearance="?android:textAppearanceSmall"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@id/textView6"
                        android:layout_below="@id/textView6"
                        android:layout_marginLeft="15.0dip"
                        android:layout_marginRight="5.0dip"
                        android:layout_marginTop="5.0dip"
                        android:clickable="true"
                        android:onClick="onClick"
                        android:text="@string/apache"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:id="@+id/textView1046"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@id/textView7"
                        android:layout_below="@id/textView7"
                        android:layout_marginBottom="12.0dip"
                        android:layout_marginLeft="15.0dip"
                        android:layout_marginRight="5.0dip"
                        android:layout_marginTop="2.0dip"
                        android:clickable="true"
                        android:onClick="onClick"
                        android:text="@string/musicg_code_google"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@android:color/black" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/LinearLayout4"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5.0dip"
                android:layout_marginRight="5.0dip"
                android:layout_marginTop="12.0dip"
                android:background="@drawable/txt_bg_hori_new5"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="0.0dip"
                android:weightSum="2.0">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="8.0dip"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:text="Bleny App"
                    android:textAppearance="?android:textAppearanceLarge"
                    android:textStyle="bold" />

                <LinearLayout
                    android:id="@+id/LinearLayout15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:layout_marginRight="5.0dip"
                    android:layout_marginTop="12.0dip"
                    android:orientation="horizontal"
                    android:padding="0.0dip"
                    android:weightSum="3.0">

                    <ImageView
                        android:id="@+id/face"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20.0dip"
                        android:layout_weight="1.0"
                        android:clickable="true"
                        android:onClick="onClick"
                        android:src="@drawable/facebook_icon" />

                    <ImageView
                        android:id="@+id/playstore1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_marginRight="20.0dip"
                        android:layout_weight="1.0"
                        android:clickable="true"
                        android:onClick="onClick"
                        android:src="@drawable/playstore_icon" />

                    <ImageView
                        android:id="@+id/twitter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_weight="1.0"
                        android:clickable="true"
                        android:onClick="onClick"
                        android:src="@drawable/twitter_icon" />
                </LinearLayout>

                <TextView
                    android:id="@+id/textView90"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="8.0dip"
                    android:layout_marginTop="12.0dip"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:text="www.blenyapp.com"
                    android:textAppearance="?android:textAppearanceMedium" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout4454"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/LinearLayout3"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="1.0dip"
                android:layout_marginRight="1.0dip"
                android:layout_marginTop="15.0dip"
                android:orientation="vertical"
                android:padding="0.0dip"
                android:weightSum="1.0">

                <TextView
                    android:id="@+id/textView10130"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1.0dip"
                    android:layout_marginTop="15.0dip"
                    android:text=""
                    android:textAppearance="?android:textAppearanceMedium" />
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>